   "source": [
    "def factorial(n):\n",
    "  if n == 0 or n == 1:\n",
    "      return 1\n",
    "  else:\n",
    "      return n * factorial(n - 1)\n",
    "user_input = int(input(\"Enter a non-negative integer: \"))\n",
    "result = factorial(user_input)\n",
    "print(\"Factorial is:\", result) "
   ]
  }
